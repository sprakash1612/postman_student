						"exec": [
							"var responseJSON = pm.response.json();\r",
							"\r",
							"// Extract the actorName from the response body and save it as a new collection variable called \"favoriteActor\"\r",
							"pm.collectionVariables.set(\"favoriteActor\", responseJSON.actorName);"
						],
